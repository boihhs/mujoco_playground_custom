    <default class="zeroth">
      <geom condim="1" contype="0" conaffinity="0" material="Torso_material"/>
      <site rgba="1 0 0 1" size="0.01" group="5"/>

      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2"/>
      </default>

      <default class="collision">
        <geom group="3" contype="2" conaffinity="1"/>
        <default class="foot">
          <geom size="0.1115 0.05 0.015" pos="0.01 0 -0.015" type="box" contype="4" conaffinity="4"/>
          <default class="foot_sphere">
            <geom contype="2" conaffinity="1" size=".01" type="sphere"/>
          </default>
        </default>
      </default>

      <!-- Global joint + actuator defaults -->
      <joint frictionloss="0.1" damping="0.1" armature="0.005"/>
      <position inheritrange="1"/>

      <!-- Shoulder -->
      <default class="shoulder">
        <joint actuatorfrclimited="true" actuatorfrcrange="-8 8" damping="3"/>
        <position kp="20"/>
        <default class="shoulder_pitch">
          <joint axis="0 0 1" range="-1.74533 1.74533"/>
        </default>
        <default class="shoulder_yaw">
          <joint axis="0 0 1" range="-1.13446 0.872665"/>
        </default>
      </default>

      <!-- Elbow -->
      <default class="elbow">
        <joint actuatorfrclimited="true" actuatorfrcrange="-8 8" damping="3"/>
        <position kp="20"/>
        <default class="elbow_yaw">
          <joint axis="0 0 1" range="-1.5708 1.5708"/>
        </default>
      </default>

      <!-- Hip -->
      <default class="hip">
        <position kp="30"/>
        <joint actuatorfrclimited="true" actuatorfrcrange="-8 8" damping="5"/>
        <default class="hip_pitch">
          <joint axis="0 0 -1" range="-1.5708 1.5708"/>
        </default>
        <default class="hip_roll">
          <joint axis="0 0 -1" range="-0.785398 0.785398"/>
        </default>
        <default class="hip_yaw">
          <joint axis="0 0 1" range="-0.0872665 1.5708"/>
        </default>
      </default>

      <!-- Knee -->
      <default class="knee">
        <position kp="25"/>
        <joint axis="0 0 -1" range="-1.5708 1.5708" actuatorfrclimited="true" actuatorfrcrange="-8 8" damping="5"/>
      </default>

      <!-- Ankle -->
      <default class="ankle">
        <position kp="25"/>
        <joint damping="5"/>
        <default class="ankle_pitch">
          <joint axis="0 0 1" range="-1.5708 1.5708" actuatorfrclimited="true" actuatorfrcrange="-8 8"/>
        </default>
      </default>

    </default>
  </default>



